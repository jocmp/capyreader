CREATE TABLE feeds (
  id INTEGER NOT NULL PRIMARY KEY,
  external_id TEXT NOT NULL,
  feed_url TEXT NOT NULL UNIQUE
);

allByID:
SELECT *
FROM feeds
WHERE id IN ?;

findByExternalID:
SELECT *
FROM feeds
WHERE external_id = ?
LIMIT 1;

create {
INSERT INTO feeds(external_id, feed_url)
VALUES (?, ?);

SELECT *
FROM feeds
WHERE id = last_insert_rowid();
}
